{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts"],
  "exclude": ["dist", "node_modules", "out"]
}
