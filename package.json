{
  "name": "reps",
  "description": "Multiplayer server for Replicache",
  "version": "0.1.0",
  "repository": "github:rocicorp/reps",
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check '**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build-reps-dts": "tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup-reps.config.js",
    "build-reps-mjs": "esbuild --bundle --target=es2018 --platform=node --external:pg-native --outfile=out/reps.mjs --format=esm src/reps.ts",
    "build-reps-cjs": "esbuild --bundle --target=es2018 --platform=node --external:pg-native --outfile=out/reps.js --format=cjs src/reps.ts",
    "build-client-dts": "tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup-client.config.js",
    "build-client-mjs": "esbuild --bundle --target=es2018 --platform=node --external:pg-native --outfile=out/client.mjs --format=esm src/client.ts",
    "build-client-cjs": "esbuild --bundle --target=es2018 --platform=node --external:pg-native --outfile=out/client.js --format=cjs src/client.ts",
    "build-reps": "npm run build-reps-dts && npm run build-reps-mjs && npm run build-reps-cjs",
    "build-client": "npm run build-client-dts && npm run build-client-mjs && npm run build-client-cjs",
    "build": "rm -rf out && npm run build-reps && npm run build-client",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/*.test.ts'",
    "prepack": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "nanoid": "^3.1.31",
    "pg": "^8.7.1",
    "pg-error": "^1.1.0",
    "tdigest": "^0.1.1",
    "ts-node": "^10.4.0",
    "ws": "^8.2.3",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.5",
    "@types/pg": "^8.6.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "esbuild": "~0.14.0",
    "eslint": "^8.3.0",
    "mocha": "^9.1.3",
    "prettier": "^2.2.1",
    "replicache": "file:replicache-8.0.0.tgz",
    "rollup": "^2.60.1",
    "rollup-plugin-dts": "^4.0.1",
    "typescript": "^4.5.2"
  },
  "main": "out/reps.js",
  "module": "out/reps.mjs",
  "types": "out/reps.d.ts",
  "exports": {
    ".": {
      "module": "./out/reps.mjs",
      "require": "./out/reps.js",
      "default": "./out/reps.mjs"
    },
    "./out/client": {
      "module": "./out/client.mjs",
      "require": "./out/client.js",
      "default": "./out/client.mjs"
    }
  },
  "files": [
    "out/reps.d.ts",
    "out/reps.js",
    "out/reps.mjs",
    "out/client.d.ts",
    "out/client.js",
    "out/client.mjs"
  ]
}
